{
  "name": "webrtc-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --hmr --proxy-config=proxy.conf.json",
    "build": "ng build --delete-output-path=false --extract-licenses=false",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:api": "tsc --project ./tsconfig.api.json --watch",
    "build:api": "tsc --project ./tsconfig.api.json",
    "build:all": "npm run clean && npm run build && npm run build:api",
    "clean": "rimraf dist/api dist/assets dist/**/*.{html,css,js,map,svg,ico,jpg,png,webp,avif,txt} dist/**/*.*.js",
    "eslint": "eslint ./ --fix",
    "eslint:no-fix": "eslint ./",
    "postinstall": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.5",
    "@angular/common": "^14.2.5",
    "@angular/compiler": "^14.2.5",
    "@angular/core": "^14.2.5",
    "@angular/forms": "^14.2.5",
    "@angular/platform-browser": "^14.2.5",
    "@angular/platform-browser-dynamic": "^14.2.5",
    "@angular/router": "^14.2.5",
    "@ngrx/store": "^14.3.2",
    "copy-to-clipboard": "^3.3.2",
    "dayjs": "^1.11.5",
    "lodash-es": "^4.17.21",
    "ng-zorro-antd": "^14.0.0",
    "pusher": "^5.1.1-beta",
    "pusher-js": "^7.4.0",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular/cli": "^14.2.5",
    "@angular/compiler-cli": "^14.2.5",
    "@babel/eslint-parser": "^7.19.1",
    "@sweet-milktea/babel-preset-sweet": "^4.0.0",
    "@types/jasmine": "^4",
    "@types/lodash-es": "^4",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.25.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jasmine-core": "^4.4.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "lint-staged": "^13.0.3",
    "next": "^12.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs,ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}