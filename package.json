{
  "name": "webrtc-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --hmr --proxy-config=proxy.conf.json",
    "build": "ng build --delete-output-path=false --optimization",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:api": "tsc --project ./tsconfig.api.json --watch",
    "build:api": "tsc --project ./tsconfig.api.json",
    "build:all": "npm run clean && npm run build && npm run build:api",
    "clean": "rimraf dist/api dist/assets dist/**/*.{html,css,js,map,svg,ico,jpg,png,webp,avif,txt} dist/**/*.*.js",
    "eslint": "eslint ./ --fix",
    "eslint:no-fix": "eslint ./",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ngrx/store": "^13.2.0",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.11.3",
    "ng-zorro-antd": "^13.2.2",
    "pusher": "^5.1.0-beta",
    "pusher-js": "^7.1.1-beta",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.0",
    "@babel/eslint-parser": "^7.18.2",
    "@sweet-milktea/babel-preset-sweet": "^3.1.9",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^13.0.1",
    "next": "^12.1.6",
    "rimraf": "^3.0.2",
    "typescript": "~4.6.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs,ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
